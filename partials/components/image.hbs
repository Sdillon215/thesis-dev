<picture{{#if class}} class="{{class}}"{{/if}}>
	<source
		srcset="
			{{#match size "32"}}
			{{img_url src size="32" format="webp"}} 32w
			{{else match size "60"}}
			{{img_url src size="32" format="webp"}} 30w,
			{{img_url src size="60" format="webp"}} 60w
			{{else match size "100"}}
			{{img_url src size="100" format="webp"}} 100w,
			{{img_url src size="200" format="webp"}} 200w
			{{else match size "200"}}
			{{img_url src size="200" format="webp"}} 200w,
			{{img_url src size="300" format="webp"}} 300w,
			{{img_url src size="400" format="webp"}} 400w
			{{else match size "300"}}
			{{img_url src size="200" format="webp"}} 200w,
			{{img_url src size="300" format="webp"}} 300w,
			{{img_url src size="400" format="webp"}} 400w,
			{{img_url src size="600" format="webp"}} 600w
			{{else match size "400"}}
			{{img_url src size="300" format="webp"}} 300w,
			{{img_url src size="400" format="webp"}} 400w,
			{{img_url src size="600" format="webp"}} 600w,
			{{img_url src size="800" format="webp"}} 800w
			{{else match size "600"}}
			{{img_url src size="300" format="webp"}} 300w,
			{{img_url src size="400" format="webp"}} 400w,
			{{img_url src size="600" format="webp"}} 600w,
			{{img_url src size="800" format="webp"}} 800w,
			{{img_url src size="1200" format="webp"}} 1200w
			{{else}}
			{{img_url src size="300" format="webp"}} 300w,
			{{img_url src size="400" format="webp"}} 400w,
			{{img_url src size="600" format="webp"}} 600w,
			{{img_url src size="800" format="webp"}} 800w,
			{{img_url src size="1200" format="webp"}} 1200w,
			{{img_url src size="1600" format="webp"}} 1600w
			{{/match}}
		"
		sizes="{{#if size}}{{sizes}}{{else}}(max-width: 450px) 92vw, (max-width: 660px) 400px, (max-width: 1300px) 580px, (max-width: 1500px) 680px, 780px{{/if}}"
		type="image/webp"
	>
	<img
		src="{{img_url src size}}"
		srcset="
			{{#match size "32"}}
			{{img_url src size="32"}} 32w
			{{else match size "60"}}
			{{img_url src size="32"}} 30w,
			{{img_url src size="60"}} 60w
			{{else match size "100"}}
			{{img_url src size="100"}} 100w,
			{{img_url src size="200"}} 200w
			{{else match size "200"}}
			{{img_url src size="200"}} 200w,
			{{img_url src size="300"}} 300w,
			{{img_url src size="400"}} 400w
			{{else match size "300"}}
			{{img_url src size="200"}} 200w,
			{{img_url src size="300"}} 300w,
			{{img_url src size="400"}} 400w,
			{{img_url src size="600"}} 600w
			{{else match size "400"}}
			{{img_url src size="300"}} 300w,
			{{img_url src size="400"}} 400w,
			{{img_url src size="600"}} 600w,
			{{img_url src size="800"}} 800w
			{{else match size "600"}}
			{{img_url src size="300"}} 300w,
			{{img_url src size="400"}} 400w,
			{{img_url src size="600"}} 600w,
			{{img_url src size="800"}} 800w,
			{{img_url src size="1200"}} 1200w
			{{else}}
			{{img_url src size="300"}} 300w,
			{{img_url src size="400"}} 400w,
			{{img_url src size="600"}} 600w,
			{{img_url src size="800"}} 800w,
			{{img_url src size="1200"}} 1200w,
			{{img_url src size="1600"}} 1600w
			{{/match}}
		"
		alt="{{#if alt}}{{alt}}{{else}}{{altElse}}{{/if}}"
		sizes="{{#if size}}{{sizes}}{{else}}(max-width: 450px) 92vw, (max-width: 660px) 400px, (max-width: 1300px) 580px, (max-width: 1500px) 680px, 780px{{/if}}"
		loading="{{#if fast}}eager{{else}}lazy{{/if}}"
	>
</picture>
